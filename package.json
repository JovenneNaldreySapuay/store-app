{
  "name": "react-shopping",
  "version": "1.0.0",
  "description": "\"A React.js e-commerce application\"",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm run start --prefix app-client",
