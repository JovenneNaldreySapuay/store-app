    "@hapi/joi": "^17.0.0",
    "@sendgrid/mail": "^6.3.1",
    "async": "^3.2.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "cloudinary": "^1.18.1",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "event-stream": "^4.0.1",
    "express": "^4.16.4",
    "express-form-data": "^2.0.10",
    "jsonwebtoken": "^8.4.0",
    "localtunnel": "^1.9.1",
    "mongoose": "^5.3.12",
    "mongoose-unique-validator": "^2.0.2",
